                        <condition name="targetUser.id" defaultValue="0" operation="eq"/>
                        <condition name="isWatch" defaultValue="0" operation="eq"/>
                        <condition name="status" defaultValue="1" operation="eq"/>
                    </query>
                </do>

            </does>
        </entity>

